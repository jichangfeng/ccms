function cmsdata_schema() {
  return array();
}

/**
 * Implementation of hook_install().
 */
function cmsdata_install() {
  $backupPOST = $_POST;
  /* core */
  _cmsdata_install_cms_translation();
  _cmsdata_install_cms_siteinfo();
  _cmsdata_install_cms_navigation();
  _cmsdata_install_cms_role();
  _cmsdata_install_cms_user_message();
  _cmsdata_install_cms_user_relationship();
  /* recommend */
  _cmsdata_install_cms_user();
  _cmsdata_install_cms_article();
  _cmsdata_install_cms_image();
  _cmsdata_install_cms_textlink();
  _cmsdata_install_cms_imagelink();
  $_POST = $backupPOST;
}
