function _cmsdata_install_createfieldcategory(){
  $mfc = new MiniFieldCategory(null);
  $result = $mfc->SaveDataToDB();
  if(is_array($result)){ _cmsdata_install_errors_print($result); }
  return $result;
}
function _cmsdata_install_createfield($cid){
  $mf = new MiniField($cid);
  $result = $mf->SaveDataToDB();
  if(is_array($result)){ _cmsdata_install_errors_print($result); }
}
function _cmsdata_install_errors_print($errors){
  foreach($errors as $value){
    drupal_set_message($value, 'error');
  }
}

/**
 * Implementation of hook_uninstall().
 */
function cmsdata_uninstall() {
  //$tablenames = array('cms_translation', 'cms_siteinfo', 'cms_navigation', 'cms_role', 'cms_user_message', 'cms_user_relationship', 'cms_user', 'cms_article', 'cms_image', 'cms_textlink', 'cms_imagelink');
  //$arrDatas = MiniFieldCategory::FilterDatasByCid(null, array(), array(), array(array('tablename', $tablenames, 'IN')), array(array('weight', 'DESC')), null, false);
  $arrDatas = MiniFieldCategory::FilterDatasByCid(null, array(), array(), array(array('id', array(1, 11), 'BETWEEN')), array(), null, false);
  foreach($arrDatas as $Data){
    MiniFieldData::DeleteDatasByCid($Data->id, array(), true);
    MiniFieldCategory::DeleteDataByCidId(null, $Data->id, true);
  }
}

